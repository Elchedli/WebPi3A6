{{form_start(form,{'attr':{'novalidate':'novalidate'}})}}
<div class="form-inline">
    <div class="form-group col-md-6">
        {{ form_widget(form.username) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.username) }}
        </div>
    </div>
    <div class="form-group col-md-6">
        {{ form_widget(form.client) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.client) }}
        </div>
    </div>
    <div class="form-group col-md-6">
        {{ form_widget(form.titreS) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.titreS) }}
        </div>
    </div>
    <div class="form-group col-md-6">
        {{ form_widget(form.dateDs) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.dateDs) }}
        </div>
    </div>
    <div class="form-group col-md-6">
        {{ form_widget(form.dateFs) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.dateFs) }}
        </div>
    </div>

    <div class="form-group col-md-6">
        {{ form_widget(form.tempsDs) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.tempsDs) }}
        </div>
    </div>

    <div class="form-group col-md-6">
        {{ form_widget(form.tempsFs) }}
        <div class="text-xs font-weight-bold text-warning text-uppercase mb-2">
            {{ form_errors(form.tempsFs) }}
        </div>
    </div>

</div>

<div style="text-align: center;">
    <br>
    <button class="btn btn-primary mt-4 pr-4 pl-4">{{ button_label|default('Enregistrer') }}</button>
</div>

<br>
{{ form_end(form) }}

