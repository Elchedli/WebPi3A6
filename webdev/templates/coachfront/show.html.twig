{% extends 'frontend/page.html.twig' %} {% block style %}  <link href="{{asset('frontend/general/css/mouradstyle.css')}}" rel="stylesheet"> {% endblock %}

{% block title %}coach{% endblock %}

{% block param %}
    <h1>coach</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Username</th>
                <td>{{ coach.username }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ coach.mail }}</td>
            </tr>
            <tr>
                <th>Date of birth</th>
                <td>{{ coach.dateN ? coach.dateN|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('coachfront_index') }}">back to list</a>
{% endblock %}
